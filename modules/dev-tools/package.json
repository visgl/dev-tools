{
  "name": "@vis.gl/dev-tools",
  "description": "Dev tools for vis.gl frameworks",
  "license": "MIT",
  "version": "1.0.0-alpha.9",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "javascript",
    "devops"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/dev-tools"
  },
  "files": [
    "src",
    "scripts",
    "templates",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
